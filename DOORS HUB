    Config.Speed.Enabled = state
    if state then
        SpeedSystem:Enable()
    else
        SpeedSystem:Disable()
    end
end, Config.Speed.Enabled)

CreateToggle("üõ°Ô∏è –ò—Å–ø–æ–ª—å–∑–æ–≤–∞—Ç—å –ê–Ω—Ç–∏—á–∏—Ç", "üîí", function(state)
    Config.Speed.UseAntiCheat = state
    
    if state then
        game:GetService("StarterGui"):SetCore("SendNotification", {
            Title = "üõ°Ô∏è –ê–ù–¢–ò–ß–ò–¢";
            Text = "‚úÖ –ó–∞—â–∏—Ç–∞ –ê–ö–¢–ò–í–ò–†–û–í–ê–ù–ê! –í—ã –≤ –±–µ–∑–æ–ø–∞—Å–Ω–æ—Å—Ç–∏.";
            Duration = 4;
        })
    else
        game:GetService("StarterGui"):SetCore("SendNotification", {
            Title = "‚ö†Ô∏è –ü–†–ï–î–£–ü–†–ï–ñ–î–ï–ù–ò–ï";
            Text = "‚ùå –ê–Ω—Ç–∏—á–∏—Ç –æ—Ç–∫–ª—é—á–µ–Ω! –í—ã—Å–æ–∫–∏–π —Ä–∏—Å–∫ –±–∞–Ω–∞!";
            Duration = 4;
        })
    end
    
    if Config.Speed.Enabled then
        SpeedSystem:Disable()
        wait(0.1)
        SpeedSystem:Enable()
    end
end, Config.Speed.UseAntiCheat)

