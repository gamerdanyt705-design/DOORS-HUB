-- –°—á–µ—Ç—á–∏–∫ ESP
local ESPCounter = Instance.new("TextLabel")
ESPCounter.Size = UDim2.new(0, 150, 0, 20)
ESPCounter.Position = UDim2.new(0, 220, 0, 45)
ESPCounter.BackgroundTransparency = 1
ESPCounter.Text = "üìä ESP: 0"
ESPCounter.TextColor3 = Color3.fromRGB(100, 200, 255)
ESPCounter.Font = Enum.Font.Gotham
ESPCounter.TextSize = 11
ESPCounter.TextXAlignment = Enum.TextXAlignment.Left
ESPCounter.Parent = Title

task.spawn(function()
    while wait(1) do
        local doorCount = 0
        local totalCount = 0
        local tracerCount = 0
        
        for _ in pairs(ESP.DoorESP) do
            doorCount = doorCount + 1
        end
        for _ in pairs(ESP.Objects) do
            totalCount = totalCount + 1
        end
        for _ in pairs(ESP.Tracers) do
            tracerCount = tracerCount + 1
        end
        
        ESPCounter.Text = string.format("üìä –î–≤–µ—Ä–µ–π: %d | –í—Å–µ–≥–æ: %d | –õ–∏–Ω–∏–π: %d", doorCount, totalCount, tracerCount)
    end
end)

-- –°—Ç–∞—Ç—É—Å –ê–Ω—Ç–∏—á–∏—Ç–∞
local AntiCheatStatus = Instance.new("TextLabel")
AntiCheatStatus.Size = UDim2.new(1, -30, 0, 25)
AntiCheatStatus.Position = UDim2.new(0, 15, 0, 70)
AntiCheatStatus.BackgroundColor3 = Color3.fromRGB(30, 30, 50)
AntiCheatStatus.Text = "üõ°Ô∏è –ê–Ω—Ç–∏—á–∏—Ç: –ù–µ–∞–∫—Ç–∏–≤–µ–Ω | –ë–µ–∑–æ–ø–∞—Å–Ω–æ—Å—Ç—å: 0%"
AntiCheatStatus.TextColor3 = Color3.fromRGB(150, 150, 150)
AntiCheatStatus.Font = Enum.Font.Gotham
AntiCheatStatus.TextSize = 12
AntiCheatStatus.Parent = Title

local AntiCheatCorner = Instance.new("UICorner")
AntiCheatCorner.CornerRadius = UDim.new(0, 8)
AntiCheatCorner.Parent = AntiCheatStatus

task.spawn(function()
    while wait(0.5) do
        if AntiCheat.Enabled and Config.Speed.Enabled then
            local safetyLevel = math.floor((1 - AntiCheat.Data.DetectionLevel) * 100)
            local statusColor = safetyLevel > 70 and Color3.fromRGB(0, 255, 100) or
                                safetyLevel > 40 and Color3.fromRGB(255, 200, 0) or
                                Color3.fromRGB(255, 100, 100)
            
            AntiCheatStatus.Text = string.format("üõ°Ô∏è –ê–Ω—Ç–∏—á–∏—Ç: –ê–∫—Ç–∏–≤–µ–Ω | –°–∫–æ—Ä–æ—Å—Ç—å: %.1f | –ë–µ–∑–æ–ø–∞—Å–Ω–æ—Å—Ç—å: %d%%", 
                AntiCheat.Data.CurrentSpeed, safetyLevel)
            AntiCheatStatus.TextColor3 = statusColor
        else
            AntiCheatStatus.Text = "üõ°Ô∏è –ê–Ω—Ç–∏—á–∏—Ç: –ù–µ–∞–∫—Ç–∏–≤–µ–Ω | –ë–µ–∑–æ–ø–∞—Å–Ω–æ—Å—Ç—å: 0%"
            AntiCheatStatus.TextColor3 = Color3.fromRGB(150, 150, 150)
        end
    end
end)

